<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!--设置圆按钮的大小-->
    <size
        android:width="20dp"
        android:height="20dp" />
    <!--填充色-->
    <solid android:color="@color/white" />
    <!--边框的颜色的宽度-->
    <stroke
        android:width="3dp"
        android:color="#00C5C8D8" />
    <!--圆角-->
    <corners android:radius="14dp" />
</shape>